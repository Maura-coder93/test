public class Main {   // bir parametrli deyer qaytaran sual
    public static void main(String[] args) {
        System.out.println("Topla"+cem());
    }
    
    public static int cem(){
        
        int x=23;
       
        
        return x;
    }
}

public class Main {    //2 parametrli deyer qaytarmayan metod // hem de deyer qaytarmayan metod sualina da aiddir
    public static void main(String[] args) { 
        myMetod(); 
        myWeb();
        
    }
    
    public static void myMetod(){
        System.out.println("Hello");
       
    }
    public static void myWeb(){
        System.out.println("World");
}
}

interface Animal {    // interface e aid sual
  public void animalSound(); // interface method (does not have a body)
  public void sleep(); // interface method (does not have a body)
}

class Pig implements Animal {
  public void animalSound() {
    System.out.println("The pig says: wee wee");
  }
  public void sleep() {
    System.out.println("Zzz");
  }
}

class Main {
  public static void main(String[] args) {
    Pig myPig = new Pig();
    myPig.animalSound();
    myPig.sleep();
  }
}



