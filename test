using System;
static void main(String[] args) { }
Console.WriteLine("Bir reqem daxil edin");

int a = int.Parse(Console.ReadLine());  

if (a % 2 == 0)
{
    Console.WriteLine("Bu reqemin son ededi cutdur");
}
else
{
    Console.WriteLine("Bu reqemin son ededi tekdir");
}

// See https://aka.ms/new-console-template for more information
using System;
static void main(String[] args) { }
Console.WriteLine("Bir reqem daxil edin");

int a = int.Parse(Console.ReadLine());
int d = a / 100;
int b = a / 10 % 10;
int c = a % 100 % 10;

if (d==b||d==c||c==d||c==b)
{
    Console.WriteLine("Daxil etdiyiniz ededin reqemleri eynidir");
}
else
{
    Console.WriteLine("Daxil etdiyiniz ededin reqemleri ferqlidir");
}


