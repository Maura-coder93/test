using System;
 
{
    Console.Clear();
    int intders;
    int ntc;
    Console.WriteLine("4 REQEMLI EDED DAXIL ET");
    intders=Convert.ToInt32(Console.ReadLine());
    ntc = intders * 20 / 100;
    ntc= ntc * 10 /100;
    Console.WriteLine("Netice-");
    Console.WriteLine(ntc);


}

