using System;
namespace CsharpString{

public class HelloWorld
{
    public static void Main(string[] args)
    {
        string str0="ice";
        string str1="cream";
        string result;
        
        result = string.Concat(str0, str1);
        Console.WriteLine(result);
        Console.ReadLine();
    }
}
}
